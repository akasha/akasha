{
  "sourceSelector": "name=common name=common_patch name=errors name=webgpu name=webgpu_patch name=minimal_events name=typedarray name=basic_types name=minimal_webgpu_integration",
  "pre": ["base_webgpu", "base_common", "base_typedarray"],
  "stages": [
    {
      "name": "MergeDocs",
      "description": "Add documentation from doc repository. This needs to be done before Flatten and renames as some documentation applies to Mixins or to elements before they are renamed",
      "config": {
        "aliases": {
          "EvalError": ["Error"],
          "RangeError": ["Error"],
          "ReferenceError": ["Error"],
          "SyntaxError": ["Error"],
          "TypeError": ["Error"],
          "URIError": ["Error"],
          "Int8Array": ["TypedArray"],
          "Int16Array": ["TypedArray"],
          "Int32Array": ["TypedArray"],
          "Uint8Array": ["TypedArray"],
          "Uint16Array": ["TypedArray"],
          "Uint32Array": ["TypedArray"],
          "Uint8ClampedArray": ["TypedArray"],
          "Float32Array": ["TypedArray"],
          "Float64Array": ["TypedArray"],
          "EXT_disjoint_timer_query_webgl2": ["EXT_disjoint_timer_query"],
          "WebGL2RenderingContext": ["WebGLRenderingContext"]
        },
        "memberAliases": {
          "WebGL2RenderingContext.uniform1f": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform1fv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform1i": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform1iv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform1ui": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform1uiv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform2f": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform2fv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform2i": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform2iv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform2ui": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform2uiv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform3f": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform3fv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform3i": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform3iv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform3ui": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform3uiv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform4f": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform4fv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform4i": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform4iv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform4ui": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniform4uiv": ["WebGLRenderingContext.uniform"],
          "WebGL2RenderingContext.uniformMatrix2fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.uniformMatrix2x3fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.uniformMatrix2x4fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.uniformMatrix3fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.uniformMatrix3x2fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.uniformMatrix3x4fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.uniformMatrix4fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.uniformMatrix4x2fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.uniformMatrix4x3fv": ["WebGLRenderingContext.uniformMatrix"],
          "WebGL2RenderingContext.vertexAttrib1f": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttrib1fv": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttrib2f": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttrib2fv": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttrib3f": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttrib3fv": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttrib4f": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttrib4fv": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttribI4i": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttribI4iv": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttribI4ui": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.vertexAttribI4uiv": ["WebGLRenderingContext.vertexAttrib"],
          "WebGL2RenderingContext.texParameterf": ["WebGLRenderingContext.texParameter"],
          "WebGL2RenderingContext.texParameteri": ["WebGLRenderingContext.texParameter"]
        },
        "createEvents": true
      }
    },
    {
      "name": "AddExtendedAttribute",
      "description": "Package up the elements defined by ECMA into a separate package to model behaviour of elemental2-core",
      "sourceSelector": "name=atomics name=errors name=symbol name=json name=math name=basic_types",
      "config": {
        "namePattern": "^.*$",
        "extendedAttribute": "JavaSubPackage=core"
      }
    },
    {
      "name": "Merge"
    },
    {
      "name": "Flatten"
    },
    {
      "name": "Jsinterop",
      "config": {
        "outputType": "j2cl",
        "extraClosureModulesToRequireInCompileTest": [
          "data/pipelines/complete_closure_modules.txt"
        ],
        "externalTypeMapping": [
          "data/pipelines/types.mapping"
        ],
        "predefinedTypeMapping": [
          "data/pipelines/common_predefined.mapping",
          "data/pipelines/minimal_webgpu_integration_predefined_java_types.mapping",
          "data/pipelines/complete_predefined.mapping"
        ],
        "outputDirectory": "akasha/webgpu-j2cl/generated/webtack",
        "packageName": "akasha"
      }
    },
    {
      "name": "Emit",
      "config": {
        "filePattern": "akasha/webgpu-j2cl/generated/webtack/main/resources/akasha/Akasha.webidl"
      }
    },
    {
      "name": "Closure",
      "config": {
        "outputDirectory": "akasha/webgpu-j2cl/generated/webtack",
        "key": "akasha/Akasha",
        "predefinedSymbolCatalogs": [
          "data/pipelines/minimal_events_predefined_js_types.types",
          "data/pipelines/minimal_webgpu_integration_predefined_js_types.types",
          "data/pipelines/predefined_js_types.types"
        ],
        "additionalExternFragments": [
          "data/pipelines/j2cl_webgpu.externs.js"
        ]
      }
    }
  ]
}
